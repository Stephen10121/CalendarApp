{
  "expo": {
    "name": "Task Giver",
    "slug": "calendar-app",
    "scheme": "com.gruzservices.calendarapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.gruzservices.calendarapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "owner": "stephen10121",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3bb9de14-47df-4341-8e8d-f41892c9b84c"
      }
    }
  }
}
